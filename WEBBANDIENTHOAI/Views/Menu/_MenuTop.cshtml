@model IEnumerable<WEBBANDIENTHOAI.Models.MFC.Category>

@if (Model != null && Model.Any())
{
    <ul class="navbar_menu">
        @foreach (var item in Model)
        {
            if (item.Alias.Equals("trang-chu"))
            {
                item.Alias = "";
            }
            <li style="margin-left: 20px;"><a href="/@item.Alias">@item.Title</a></li>
        }

